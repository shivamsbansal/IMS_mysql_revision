<% provide(:title, 'Categories') %>
<h1>Categories</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Edit</th>
    </tr>
  </thead>
<% if @list!=nil %>
  <% @list.each do |list| %>
    <tr>
      <td><%= list.nameCategory %></td>
      <td><%=link_to "edit", "/items/edit_category/#{list.id}" %></td>
    </tr>     
  <% end %>
<% end %>

</table>